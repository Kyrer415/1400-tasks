#include <iostream>
#include <cmath>

using namespace std;

int main() {
 double x, y;
    cout << "Введите значение x: ";
    cin >> x;
    cout << "Введите значение y: ";
    cin >> y;

    
    double denominator_z = y + sqrt(x * x + 10);
    double fraction_z = (x + (2 + y) / (x * x)) / (1 / denominator_z);
    double z = fraction_z;

    
    double q = 7.25 * sin(x - fabs(y));

    cout << "Значение z: " << z << endl;
    cout << "Значение q: " << q << endl;
    return 0;
}
