#include <iostream>
#include <cmath>

using namespace std;

int main() {
double e, f, g, h;
    cout << "Введите значение e: ";
    cin >> e;
    cout << "Введите значение f: ";
    cin >> f;
    cout << "Введите значение g: ";
    cin >> g;
    cout << "Введите значение h: ";
    cin >> h;
    
    double a = (e + f) / 3;

    double b = fabs(h * h - g);

    double under_root = pow(g - h, 2) - 3 * sin(e);
    if (under_root < 0) {
        cout << "Ошибка: подкоренное выражение отрицательно!" << endl;
        return 1;
    }
    double c = sqrt(under_root);

    cout << "Значение a: " << a << endl;
    cout << "Значение b: " << b << endl;
    cout << "Значение c: " << c << endl;
    return 0;
}
