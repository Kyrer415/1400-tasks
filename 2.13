#include <iostream>
#include <cmath>

using namespace std;

int main() {
  double a, b;
    cout << "Введите коэффициенты a и b (a ≠ 0): ";
    cin >> a >> b;

    if (a == 0) {
        cout << "Ошибка: коэффициент a не должен быть равен нулю!" << std::endl;
        return 1;
    }

    double x = -b / a;
    cout << "Решение уравнения: x = " << x << std::endl;

    return 0;
}
